<li id="<%= feed_item.id %>">
	<%= link_to gravatar_for(feed_item.master), feed_item.master %>
		<span class="master">
			<%= link_to feed_item.master.name, feed_item.master %>
		</span>	
		<span class="content"><%= feed_item.content %></span>
		<span class="timestamp">
			Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
		</span>
	<%= render 'shared/delete_links', object: feed_item %>
</li>